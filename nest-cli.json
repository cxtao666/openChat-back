{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/meeting/src",
  "monorepo": true,
  "root": "apps/meeting",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/meeting/tsconfig.app.json",
    "assets": [
      "**/*.proto"
    ],
    "watchAssets": true
  },
  "projects": {
    "meeting": {
      "type": "application",
      "root": "apps/meeting",
      "entryFile": "main",
      "sourceRoot": "apps/meeting/src",
      "compilerOptions": {
        "tsConfigPath": "apps/meeting/tsconfig.app.json"
      }
    },
    "nest-service": {
      "type": "application",
      "root": "apps/nest-service",
      "entryFile": "main",
      "sourceRoot": "apps/nest-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/nest-service/tsconfig.app.json"
      }
    },
    "meeting-chat": {
      "type": "application",
      "root": "apps/meeting-chat",
      "entryFile": "main",
      "sourceRoot": "apps/meeting-chat/src",
      "compilerOptions": {
        "tsConfigPath": "apps/meeting-chat/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "document": {
      "type": "application",
      "root": "apps/document",
      "entryFile": "main",
      "sourceRoot": "apps/document/src",
      "compilerOptions": {
        "tsConfigPath": "apps/document/tsconfig.app.json"
      }
    },
    "live": {
      "type": "application",
      "root": "apps/live",
      "entryFile": "main",
      "sourceRoot": "apps/live/src",
      "compilerOptions": {
        "tsConfigPath": "apps/live/tsconfig.app.json"
      }
    },
    "question": {
      "type": "application",
      "root": "apps/question",
      "entryFile": "main",
      "sourceRoot": "apps/question/src",
      "compilerOptions": {
        "tsConfigPath": "apps/question/tsconfig.app.json"
      }
    },
    "admin": {
      "type": "application",
      "root": "apps/admin",
      "entryFile": "main",
      "sourceRoot": "apps/admin/src",
      "compilerOptions": {
        "tsConfigPath": "apps/admin/tsconfig.app.json"
      }
    },
    "api-getway": {
      "type": "application",
      "root": "apps/api-getway",
      "entryFile": "main",
      "sourceRoot": "apps/api-getway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api-getway/tsconfig.app.json"
      }
    }
  }
}